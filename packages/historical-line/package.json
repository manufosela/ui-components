{
  "name": "@manufosela/historical-line",
  "version": "2.0.0",
  "description": "A horizontal timeline visualization web component for displaying historical data, built with Lit",
  "license": "MIT",
  "author": "manufosela",
  "type": "module",
  "main": "src/historical-line.js",
  "module": "src/historical-line.js",
  "exports": {
    ".": {
      "import": "./src/historical-line.js"
    },
    "./historical-line.js": {
      "import": "./src/historical-line.js"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
