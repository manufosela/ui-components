  "bugs": {
    "url": "https://github.com/manufosela/ui-components/issues"
  },
  "homepage": "https://github.com/manufosela/ui-components/tree/main/packages/circle-percent#readme",
  "dependencies": {
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@open-wc/testing": "^4.0.0",
    "@web/dev-server": "^0.4.6",
    "@web/test-runner": "^0.19.0",
    "eslint": "^8.57.0",
    "prettier": "^3.4.2"
