  "description": "A circular progress indicator web component with animated SVG arc and customizable styling, built with Lit",
  "license": "MIT",
  "author": "manufosela",
  "type": "module",
  "main": "src/circle-percent.js",
  "module": "src/circle-percent.js",
  "exports": {
    ".": {
      "import": "./src/circle-percent.js"
    },
    "./circle-percent.js": {
      "import": "./src/circle-percent.js"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
