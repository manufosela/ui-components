{
  "name": "@manufosela/calendar-inline",
  "version": "2.0.0",
  "description": "Inline calendar web component with horizontal scrolling",
  "license": "Apache-2.0",
  "author": "manufosela",
  "type": "module",
  "main": "./src/calendar-inline.js",
