{
  "name": "@manufosela/lcd-digit",
  "version": "2.0.1",
  "description": "LCD-style digit display web component with Lit",
  "license": "MIT",
  "author": "manufosela",
  "type": "module",
  "main": "./src/lcd-digit.js",
  "module": "./src/lcd-digit.js",
  "types": "./src/lcd-digit.d.ts",
  "exports": {
    ".": {
      "types": "./src/lcd-digit.d.ts",
      "import": "./src/lcd-digit.js"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "start": "web-dev-server --node-resolve --open demo/ --watch",
    "test": "web-test-runner",
