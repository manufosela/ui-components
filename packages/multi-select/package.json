  "homepage": "https://github.com/manufosela/ui-components/tree/main/packages/multi-select#readme",
  "bugs": {
    "url": "https://github.com/manufosela/ui-components/issues"
  },
  "dependencies": {
    "lit": "^3.2.1"
  },
  "customElements": "custom-elements.json"
}
