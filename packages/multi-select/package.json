{
  "name": "@manufosela/multi-select",
  "version": "2.1.0",
  "description": "Multi-select dropdown web component with checkboxes",
  "license": "MIT",
  "author": "manufosela",
  "type": "module",
  "main": "./src/multi-select.js",
  "module": "./src/multi-select.js",
  "types": "./src/multi-select.d.ts",
  "exports": {
    ".": {
      "types": "./src/multi-select.d.ts",
      "import": "./src/multi-select.js"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
